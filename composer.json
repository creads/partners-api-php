{
    "name": "creads/partners-sdk-php",
    "type": "library",
    "description": "A simple PHP client and CLI for Creads Partners API",
    "homepage": "http://www.creads-partners.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Damien Pitard",
            "email": "d.pitard@creads.org"
        },
        {
            "name": "Alexis Stefanski",
            "email": "a.stefanski@creads.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "Creads\\Partners\\": "src/"
        }
    },
    "require": {
        "php": ">=7.1",
        "guzzlehttp/guzzle": "^6.0",
        "symfony/console": "^2.7",
        "flow/jsonpath": "^0.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "bin": [
        "bin/partners"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
