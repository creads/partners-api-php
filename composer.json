{
    "name": "creads/partners-sdk-php",
    "type": "library",
    "description": "A simple PHP client and CLI for Creads Partners API",
    "homepage": "http://www.creads-partners.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Damien Pitard",
            "email": "d.pitard@creads.org"
        },
        {
            "name": "Alexis Stefanski",
            "email": "a.stefanski@creads.org"
        }
    ],
    "require": {
        "php": ">=7.1",
        "guzzlehttp/guzzle": "^6.2",
        "symfony/console": "^2.8",
        "flow/jsonpath": "^0.3.0"
    },
    "bin": [
        "bin/partners"
    ],
    "autoload": {
        "psr-4": {
            "Creads\\Partners\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "2.x-dev",
            "dev-master": "2.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
