{
    "name": "creads/partners-sdk-php",
    "type": "library",
    "description": "A simple PHP client and CLI for Creads Partners API",
    "homepage": "http://www.creads-partners.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Damien Pitard",
            "email": "d.pitard@creads.org"
        },
        {
            "name": "Alexis Stefanski",
            "email": "a.stefanski@creads.org"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "guzzlehttp/guzzle": "^6.0",
        "symfony/console": ">=2.7",
        "flow/jsonpath": "^0.3.0"
    },
    "bin": [
        "bin/partners"
    ],
    "autoload": {
        "psr-4": {
            "Creads\\Partners\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
